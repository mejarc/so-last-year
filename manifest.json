{
  "manifest_version": 2,

  "name": "That's SO Last Year",
  "description": "This extension will dim out Stack Overflow answers older than a year.",
  "version": "0.1.0",
  "browser_action": {
    "default_icon": "icon.png"
  },
  "background": {
    "scripts": [ "event.js"],
    "persistent": false
    },
  "web_accessible_resources": [
    "so-last-year.css"
  ],
  "permissions": [ 
    "http://*.stackoverflow.com/*",
    "contextMenus"
  ],
  "icons": {
    "16": "icon-context.png"
  }
}